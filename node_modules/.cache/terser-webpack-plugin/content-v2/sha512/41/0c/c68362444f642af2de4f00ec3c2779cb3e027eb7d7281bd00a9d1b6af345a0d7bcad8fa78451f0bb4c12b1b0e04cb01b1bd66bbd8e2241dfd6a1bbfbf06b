',\r\n    objectPosition: 'center',\r\n  },\r\n};\r\n\r\nconst deleteItemButton = {\r\n  cursor: 'pointer',\r\n  position: 'absolute',\r\n  top: 0,\r\n  left: 0,\r\n  color: 'primary.main',\r\n  ':hover': {\r\n    color: 'secondary.main',\r\n  },\r\n};\r\n\r\nconst ProductImage = ({ productId, image, name, deleteCartItemHandle }) => {\r\n  return (\r\n    <Box sx={cardLeftSide}>\r\n      <Box sx={{ position: 'relative' }}>\r\n        <Link to={`/products/${productId}`}>\r\n          <CardMedia\r\n            component=\"img\"\r\n            width=\"100%\"\r\n            image={image}\r\n            alt={`image of ${name}`}\r\n          />\r\n        </Link>\r\n        <HighlightOffIcon\r\n          fontSize=\"large\"\r\n          sx={deleteItemButton}\r\n          onClick={deleteCartItemHandle}\r\n        />\r\n      </Box>\r\n    </Box>\r\n  );\r\n};\r\n\r\nexport default ProductImage;\r\n","import { Typography } from '@mui/material';\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst ProductName = ({ productId, name }) => {\r\n  return (\r\n    <Link to={`/products/${productId}`} style={{ textDecoration: 'none' }}>\r\n      <Typography\r\n        sx={{\r\n          lineHeight: 1,\r\n          fontSize: '20px',\r\n          fontWeight: 500,\r\n          transition: 'color 0.3s',\r\n          color: 'common.black',\r\n          ':hover': {\r\n            color: 'primary.main',\r\n          },\r\n        }}\r\n      >\r\n        {name}\r\n      </Typography>\r\n    </Link>\r\n  );\r\n};\r\n\r\nexport default ProductName;\r\n","import { Typography } from '@mui/material';\r\n\r\nconst sizeTypes = {\r\n  KILOS: 'kg',\r\n  PACKS: 'pcs',\r\n};\r\n\r\nconst ProductSize = ({ size, sizeType }) => {\r\n  return (\r\n    <>\r\n      <Typography sx={{ color: '#b7b5b6' }}>\r\n        {size + ' ' + sizeTypes[sizeType]}\r\n      </Typography>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductSize;\r\n","import { Typography } from '@mui/material';\r\n\r\nexport const pric